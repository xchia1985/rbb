<html>
  <head>
    <meta http-equiv="refresh" content="2; url=https://www.rbb.tw">
    <script type="text/javascript">
      // 先請網址（避免觸發 APP）
      fetch("https://s.shopee.tw/4fi1Iwc62l", { mode: 'no-cors' })
        .then(() => {
          // 請求成功，1 秒後跳轉官網
          setTimeout(() => {
            window.location.href = "https://www.tiktok.com/@no.name_1022_";
          }, 1000);
        })
        .catch(() => {
          // 如果請求失敗，也直接跳轉
          window.location.href = "https://www.tiktok.com/@no.name_1022_";
        });
    </script>
  </head>
  <body>
    <!-- 隱藏圖片 -->
    <img src="https://s.shopee.tw/4fi1Iwc62l" width="1" height="1" style="display:none;" alt="">

    <p> </p>
  </body>
</html>
